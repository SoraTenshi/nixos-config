{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "filler": "<#565f89,transparent>\u2500</>",
      "segments": [
        {
          "foreground": "#565f89",
          "style": "plain",
          "template": "\u250c",
          "type": "text"
        },
        {
          "background": "#565f89",
          "foreground": "#9ab1fa",
          "leading_diamond": "\u2500\u2500[ ",
          "style": "diamond",
          "template": "<,transparent>{{ .Icon }}</>",
          "trailing_diamond": " ]",
          "type": "os"
        },
        {
          "background": "#565f89",
          "foreground": "#9ab1fa",
          "leading_diamond": "\u2500\u2500[ ",
          "style": "diamond",
          "template": "<,transparent>{{ .Path }}</>",
          "trailing_diamond": " ]",
          "properties": {
            "style": "full"
          },
          "type": "path"
        },
        {
          "background": "#565f89",
          "foreground": "#9ab1fa",
          "leading_diamond": "\u2500\u2500[ ",
          "trailing_diamond": " ]",
          "style": "diamond",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": "<,transparent>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf040 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf067 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf187 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }}{{ if .UpstreamGone }}<#c678dd> \uf12a#{{ end }}",
          "type": "git"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "newline": false,
      "filler": "<#565f89,transparent>\u2500</>",
      "segments": [
        {
          "type": "battery",
          "leading_diamond": "[ ",
          "style": "diamond",
          "trailing_diamond": " ]\u2500\u2500",
          "background": "#565f89",
          "foreground": "#9ab1fa",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#56d8f4{{end}}",
            "{{if eq \"Discharging\" .State.String}}#c678dd{{end}}",
            "{{if eq \"Full\" .State.String}}#9ab1fa{{end}}"
          ],
          "template": "<,transparent>{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}\uf240</>",
          "properties": {
            "discharging_icon": "\uf243 ",
            "charging_icon": "\uf0e7 ",
            "charged_icon": "\uf240 "
          }
        },
        {
          "background": "#565f89",
          "foreground": "#9ab1fa",
          "leading_diamond": "[ ",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": "<,transparent>{{ .CurrentDate | date .Format }}</>",
          "trailing_diamond": " ]",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#565f89",
          "style": "plain",
          "template": "\u2514",
          "type": "text"
        },
        {
          "background": "#565f89",
          "foreground": "#9ab1fa",
          "leading_diamond": "\u2500\u2500[ ",
          "style": "diamond",
          "template": "<,transparent>{{ .UserName }}</><#565f89,transparent>@</><,transparent>{{ .HostName }}</>",
          "trailing_diamond": " ]",
          "type": "text"
        },
        {
          "foreground": "#9ab1fa",
          "style": "plain",
          "template": "<#565f89>\u2500\u2500</>\u276f\u276f",
          "type": "text"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 2,
  "transient_prompt": {
    "background": "transparent",
    "foreground_templates": [
      "{{ne .Code 0}} #c678dd{{end}}"
    ],
    "foreground": "#9ab1fa",
    "template": "\u276f\u276f "
  }
}
