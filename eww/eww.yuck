(defwidget main_bar []
	(box :class "bar"
		 :orientation "v"
))

(defwindow main_bar
	:monitor 0
	:geometry (geometry :x "0"
						:y "0"
						:width "30px"
						:height "99%"
						:anchor "center left")
	:windowtype "dock"
	:wm-ignore true
	:reserve (struts :side "left" :distance "35px")
	(main_bar)
)
