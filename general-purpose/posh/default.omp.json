{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "foreground": "#414868",
          "style": "plain",
          "template": "\u250c",
          "type": "text"
        },
        {
          "background": "#414868",
          "foreground": "#7dcfff",
          "leading_diamond": "\u2500[",
          "style": "diamond",
          "template": "\uf313",
          "trailing_diamond": "]\u2500",
          "type": "text"
        },
        {
          "background": "#414868",
          "foreground": "#7dcfff",
          "leading_diamond": "\u2500[",
          "properties": {
            "time_format": "13:37"
          },
          "style": "diamond",
          "template": "{{ .CurrentDate | date .Format }}",
          "trailing_diamond": "]\u2500",
          "type": "time"
        },
        {
          "background": "#414868",
          "foreground": "#7dcfff",
          "leading_diamond": "\u2500[",
          "style": "diamond",
          "template": "{{ .Path }}",
          "trailing_diamond": "]\u2500",
          "type": "text"
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#7dcfff",
    "template": "❯❯ "
  },
  "final_space": true,
  "version": 2
}
